{
  "name": "unplugin-vue",
  "version": "3.0.0-beta.0",
  "packageManager": "pnpm@7.6.0",
  "description": "Transform Vue 3 SFC to JavaScript.",
  "keywords": [
    "vue",
    "sfc",
    "unplugin",
    "vite",
    "webpack",
    "rollup",
    "esbuild"
  ],
  "license": "MIT",
  "homepage": "https://github.com/sxzz/unplugin-vue#readme",
  "bugs": {
    "url": "https://github.com/sxzz/unplugin-vue/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sxzz/unplugin-vue.git"
  },
  "author": "三咲智子 <sxzz@sxzz.moe>",
  "files": [
    "dist",
    "*.d.ts"
  ],
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./vite": {
      "require": "./dist/vite.js",
      "import": "./dist/vite.mjs"
    },
    "./webpack": {
      "require": "./dist/webpack.js",
      "import": "./dist/webpack.mjs"
    },
    "./rollup": {
      "require": "./dist/rollup.js",
      "import": "./dist/rollup.mjs"
    },
    "./esbuild": {
      "require": "./dist/esbuild.js",
      "import": "./dist/esbuild.mjs"
    },
    "./*": "./*"
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*"
      ]
    }
  },
  "scripts": {
    "test": "vitest",
    "build": "tsup && tsx scripts/postbuild.mts",
    "release": "bumpp",
    "lint": "eslint . --ext .vue,.ts --max-warnings 0",
    "prepublishOnly": "pnpm run build"
  },
  "peerDependencies": {
    "vite": "^3.0.0",
    "vue": "^3.2.25"
  },
  "peerDependenciesMeta": {
    "vite": {
      "optional": true
    }
  },
  "dependencies": {
    "debug": "^4.3.4",
    "tsx": "^3.8.0",
    "unplugin": "^0.7.2"
  },
  "devDependencies": {
    "@jridgewell/gen-mapping": "^0.3.2",
    "@jridgewell/trace-mapping": "^0.3.14",
    "@sxzz/eslint-config-prettier": "^2.3.1",
    "@sxzz/eslint-config-ts": "^2.3.1",
    "@sxzz/eslint-config-vue": "^2.3.1",
    "@types/debug": "^4.1.7",
    "@types/node": "*",
    "@vitejs/plugin-vue": "3.0.1",
    "@vitest/ui": "0.19.0",
    "bumpp": "^8.2.1",
    "esbuild": "^0.14.49",
    "eslint": "^8.19.0",
    "eslint-define-config": "^1.5.1",
    "fast-glob": "^3.2.11",
    "rollup": "^2.76.0",
    "rollup-plugin-esbuild": "^4.9.1",
    "slash": "^4.0.0",
    "source-map": "~0.6.1",
    "tsup": "^6.1.3",
    "typescript": "^4.7.4",
    "vite": "^3.0.0",
    "vitest": "0.19.0",
    "vue": "^3.2.37",
    "webpack": "^5.73.0"
  },
  "engines": {
    "node": ">=14.17.0"
  },
  "volta": {
    "node": "16.14.2"
  }
}
