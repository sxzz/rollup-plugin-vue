{
  "name": "unplugin-vue",
  "version": "4.0.2-beta.1",
  "packageManager": "pnpm@7.21.0",
  "description": "Transform Vue 3 SFC to JavaScript.",
  "keywords": [
    "vue",
    "sfc",
    "unplugin",
    "vite",
    "webpack",
    "rollup",
    "esbuild"
  ],
  "license": "MIT",
  "homepage": "https://github.com/sxzz/unplugin-vue#readme",
  "bugs": {
    "url": "https://github.com/sxzz/unplugin-vue/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sxzz/unplugin-vue.git"
  },
  "author": "三咲智子 <sxzz@sxzz.moe>",
  "files": [
    "dist",
    "*.d.ts"
  ],
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./vite": {
      "types": "./dist/vite.d.ts",
      "require": "./dist/vite.js",
      "import": "./dist/vite.mjs"
    },
    "./webpack": {
      "types": "./dist/webpack.d.ts",
      "require": "./dist/webpack.js",
      "import": "./dist/webpack.mjs"
    },
    "./rollup": {
      "types": "./dist/rollup.d.ts",
      "require": "./dist/rollup.js",
      "import": "./dist/rollup.mjs"
    },
    "./esbuild": {
      "types": "./dist/esbuild.d.ts",
      "require": "./dist/esbuild.js",
      "import": "./dist/esbuild.mjs"
    },
    "./*": "./*"
  },
  "scripts": {
    "test": "vitest",
    "dev": "tsup --watch",
    "build": "tsup && tsx scripts/postbuild.mts",
    "release": "bumpp",
    "lint": "eslint . --ext .vue,.ts --max-warnings 0",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "pnpm run build"
  },
  "peerDependencies": {
    "vite": "^4.0.0",
    "vue": "^3.2.25"
  },
  "peerDependenciesMeta": {
    "vite": {
      "optional": true
    }
  },
  "dependencies": {
    "debug": "^4.3.4",
    "tsx": "^3.12.2",
    "unplugin": "~1.0.1",
    "vite": "^4.0.4"
  },
  "devDependencies": {
    "@jridgewell/gen-mapping": "^0.3.2",
    "@jridgewell/trace-mapping": "^0.3.17",
    "@sxzz/eslint-config-prettier": "^2.4.4",
    "@sxzz/eslint-config-vue": "^2.4.4",
    "@types/debug": "^4.1.7",
    "@types/node": "^18.11.18",
    "@vitejs/plugin-vue": "4.0.0",
    "@vitest/ui": "0.28.3",
    "bumpp": "^9.0.0",
    "esbuild": "0.17.4",
    "eslint": "^8.32.0",
    "eslint-define-config": "^1.14.0",
    "fast-glob": "^3.2.12",
    "rollup": "^3.11.0",
    "rollup-plugin-esbuild": "^5.0.0",
    "slash": "^5.0.0",
    "source-map": "~0.6.1",
    "tsup": "^6.5.0",
    "typescript": "^5.0.0-beta",
    "vite": "^4.0.0",
    "vitest": "0.28.3",
    "vue": "^3.2.45",
    "webpack": "^5.75.0"
  },
  "engines": {
    "node": ">=14.17.0"
  },
  "volta": {
    "node": "16.14.2"
  }
}
