{
  "name": "unplugin-vue",
  "version": "1.0.0",
  "description": "Transform Vue 3 SFC to JavaScript.",
  "keywords": [
    "vue",
    "sfc",
    "unplugin",
    "vite",
    "webpack",
    "rollup",
    "esbuild"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sxzz/unplugin-vue.git"
  },
  "author": "三咲智子 <sxzz@sxzz.moe>",
  "files": [
    "dist",
    "*.d.ts"
  ],
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./vite": {
      "require": "./dist/vite.js",
      "import": "./dist/vite.mjs"
    },
    "./webpack": {
      "require": "./dist/webpack.js",
      "import": "./dist/webpack.mjs"
    },
    "./rollup": {
      "require": "./dist/rollup.js",
      "import": "./dist/rollup.mjs"
    },
    "./esbuild": {
      "require": "./dist/esbuild.js",
      "import": "./dist/esbuild.mjs"
    },
    "./*": "./*"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "vitest",
    "build": "tsup && esno scripts/postbuild.ts",
    "release": "standard-version",
    "lint": "eslint . --ext .vue,.ts --max-warnings 0",
    "prepublishOnly": "pnpm run build"
  },
  "engines": {
    "node": ">=12.20.0"
  },
  "bugs": {
    "url": "https://github.com/sxzz/unplugin-vue/issues"
  },
  "homepage": "https://github.com/sxzz/unplugin-vue#readme",
  "peerDependencies": {
    "vue": "^3.2.25"
  },
  "dependencies": {
    "@rollup/pluginutils": "^4.1.2",
    "debug": "^4.3.3",
    "hash-sum": "^2.0.0",
    "querystring": "^0.2.1",
    "source-map": "^0.6.1",
    "unplugin": "^0.4.0",
    "vite": "^2.8.5"
  },
  "devDependencies": {
    "@sxzz/eslint-config-prettier": "^1.6.1",
    "@sxzz/eslint-config-ts": "^1.6.2",
    "@sxzz/eslint-config-vue": "^1.6.4",
    "@types/debug": "^4.1.7",
    "@types/hash-sum": "^1.0.0",
    "@types/node": "*",
    "@vitejs/plugin-vue": "^2.2.4",
    "@vitest/ui": "0.6.1",
    "esbuild": "^0.14.23",
    "eslint": "^8.10.0",
    "eslint-define-config": "^1.2.5",
    "esno": "^0.14.1",
    "fast-glob": "^3.2.11",
    "rollup": "^2.68.0",
    "slash": "^4.0.0",
    "standard-version": "^9.3.2",
    "tsup": "^5.11.13",
    "typescript": "^4.6.2",
    "vitest": "0.6.1",
    "vue": "^3.2.31"
  }
}
